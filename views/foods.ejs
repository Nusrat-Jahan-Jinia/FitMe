<%- include('./partials/header.ejs'); %>
    <div class="manageUser-container">
      <div id="title">
        <h2>Manage Users</h2>
      </div>

      <div class="new-message-container new-user">
        <a href="#" onclick="openModal()">+</a>
      </div>

      <div id="users-table">
        <table>
          <thead>
            <tr>
              <th>ID</th>
              <th>Name</th>
              <th>Scitific Name</th>
              <th>Description</th>
              <th>Food Value</th>
            </tr>
          </thead>
          <tbody id="users-table">
            <% foods.forEach(function(food) { %>
              <tr id="<%= food._id %>">
                <td class="name">
                  <span><%= food.id %></span>
                  <span><%= food.name %></span>
                </td>
                <td> <span><%= food.s_name %></span></td>
                <td> <span><%= food.des %></span></td>
                <td> <span><%= food.value %></span></td>
              </tr>
            <% }); %>
          </tbody>
        </table>
      </div>
    </div>


  </body>
</html>
