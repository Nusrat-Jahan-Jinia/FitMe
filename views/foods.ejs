<%- include('./partials/header.ejs'); %>
    <div class="manageUser-container">
      <div id="title">
        <h2>Manage Users</h2>
      </div>

      <div class="new-message-container new-user">
        <a href="#" onclick="openModal()">+</a>
      </div>

      <div id="users-table">
        <table>
          <thead>
            <tr>
              <th>Name</th>
              <th>Email</th>
              <th>Manage</th>
            </tr>
          </thead>
          <tbody id="users-table">
            <% foods.forEach(function(user) { %>
              <tr id="<%= user._id %>">
                <td class="name">
                  
                  <span><%= user.name %></span>
                </td>
                <td> <span><%= user.name %></span></td>
              </tr>
            <% }); %>
          </tbody>
        </table>
      </div>
    </div>


  </body>
</html>
